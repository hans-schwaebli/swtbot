         id="coretestapplication"
         point="org.eclipse.core.runtime.applications">
      <application>
         <run
               class="org.eclipse.swtbot.eclipse.headless.CoreTestApplication">
         </run>
      </application>
   </extension>
   <extension
         id="swtbottestapplication"
         point="org.eclipse.core.runtime.applications">
      <application>
         <run
               class="org.eclipse.swtbot.eclipse.headless.UITestApplication">
            <parameter
                  name="productInfo"
                  value="product.ini">
            </parameter>
         </run>
      </application>
   </extension>

</plugin>
